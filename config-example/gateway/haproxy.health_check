#!/bin/bash
proxy_tunnel_health=$(curl -o /dev/null -s -w "%{http_code}" $3:$4/_proxy_tunnel_health)
if [ $proxy_tunnel_health != "200" ]; then
    exit 1
fi

proxy_tunnel_health=$(curl -o /dev/null -s -w "%{http_code}" $3:$4/_proxy_api_health)
if [ $proxy_tunnel_health != "200" ]; then
    exit 1
fi

proxy_tunnel_health=$(curl -o /dev/null -s -w "%{http_code}" $3:$4/_guacamole_health)
if [ $proxy_tunnel_health != "200" ]; then
    exit 1
fi
